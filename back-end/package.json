        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "10.4.20",
        "@nestjs/config": "4.0.2",
        "@nestjs/core": "10.4.20",
        "@nestjs/jwt": "11.0.0",
        "@nestjs/passport": "11.0.5",
        "@nestjs/platform-express": "10.4.20",
        "@nestjs/platform-socket.io": "10.4.19",
